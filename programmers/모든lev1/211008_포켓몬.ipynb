    "\n",
    "def solution(nums):\n",
    "    answer = 0\n",
    "    length = len(nums) // 2\n",
    "    nums.sort()\n",
    "    last = 0\n",
    "    \n",
    "    for value in nums :\n",
    "        if(value != last and answer < length):\n",
    "            answer +=1\n",
    "            last = value\n",
    "            \n",
    "    return answer\n",
    "    "
   ]
